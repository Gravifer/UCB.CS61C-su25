{
    "files.exclude": {
        "**/.git": false,
        "**/.gitattributes": false,
        "**/.gitignore": false,
        "**/.gitmodules": false,
        "**/.hg": false,
        "**/.svn": false,
        "**/app.json": false,
        "**/babel.config.js": false,
        "**/CVS": false,
        "**/Cargo.toml": false,
        "**/.circleci": false,
        "**/.classpath": false,
        "**/CMakeLists.txt": false,
        "**/.codecov.yml": false,
        "**/.conda": false,
        "**/conda-environment.yml": false,
        "**/*.crswap": false,
        "**/.deepsource.toml": false,
        "**/.DS_Store": false,
        "**/desktop.ini": false,
        "**/.editorconfig": false,
        "**/.expo-shared": false,
        "**/.factorypath": false,
        "**/.github": false,
        "**/.history": false,
        "**/hooks": false,
        "**/.ipynb_checkpoints": false,
        "**/.JuliaFormatter.toml": false,
        "**/.licrc": false,
        "**/LICENSE": false,
        "**/*.lock": false,
        "**/.metals": false,
        "**/misc": false,
        "**/Manifest.toml": false,
        "**/node_modules": false,
        "**/*.olean": false,
        "**/package.json": false,
        "**/package-lock.json": false,
        "**/pnpm-lock.yaml": false,
        "**/pnpm-workspace.yaml": false,
        "**/playwright.config.ts": false,
        "**/pom.xml": false,
        "**/Procfile": false,
        "**/.project": false,
        "**/Project.toml": false,
        "**/README.md": false,
        "**/README.ipynb": false,
        "**/renovate.json": false,
        "**/.ruby-lsp": false,
        "**/.settings": false,
        "**/setup.py": false,
        "**/submodules": false,
        "**/target": false,
        "**/test": false,
        "**/Thumbs.db": false,
        "**/tsconfig.json": false,
        "*.tsx": false,
        "**/.vscode": false,
        "**/.wakatime-project": false
    },
    "explorerExclude.backup": {},
    "files.associations": {
        ".ghci": "haskell",
        "*.wls": "wolfram",
        "*.m": "matlab",
        ".vimrc": "viml",
        ".viminfo": "viml",
        ".wakatime-project": "plaintext",
        ".wegorc": "shellscript",
        ".gitignore": "ignore",
        ".prettierrc": "json",
        "*.scm": "scheme",
        "string.h": "c"
    },
    "c-cpp-flylint.language": "c",
    "c-cpp-flylint.cppcheck.enable": false,
    "c-cpp-flylint.standard": [
        "c11",
        "c17",
        "c23"
    ],
    "c-cpp-flylint.clang.standard": [
        "c11",
        "c17",
        "c23"
    ],
    "better-comments.languages": [
        {
        "id": "riscv", // (Required) Language ID
        "comments": { "lineComment": "#"}, // (Optional) Comment Syntax
        "embeddedLanguages": [], // (Optional) Embedded Languages. Example for HTML: ["css", "javascript"]
        "useDocComment": false // (Optional) Use Doc Comments
        }
    ],
    "material-icon-theme.folders.customClones": [
        {
            "name": "test-src",
            "base": "src",
            "color": "teal-A700",
            "folderNames": ["test-src"]
        },
        {
            "name": "coverage-src",
            "base": "src",
            "color": "teal-400",
            "folderNames": ["coverage-src"]
        },
        {
            "name": "projs",
            "base": "components",
            "color": "blue-600",
            "folderNames": ["projs"]
        },
        {
            "name": "labs",
            "base": "components",
            "color": "green-500",
            "folderNames": ["labs"]
        },
        {
            "name": "proj",
            "base": "project",
            "color": "blue-600",
            "folderNames": ["proj1", "proj2"],
        },
        {
            "name": "lab",
            "base": "mock",
            "color": "green-500",
            "folderNames": ["lab00", "lab01", "lab02", "lab03", "lab04", "lab05", "lab06", "lab07", "lab08"],
        }
    ]
}
